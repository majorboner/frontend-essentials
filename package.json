	"main": "index.js",
	"scripts": {
		"start": "webpack serve --env port=3000",
		"start:vite": "vite",
		"start:dev:server": "node ./json-server/index.js",
		"start:dev": "concurrently \"npm run start\" \"npm run start:dev:server\"",
		"start:dev:vite": "concurrently \"npm run start:vite\" \"npm run start:dev:server\"",
		"build:prod": "webpack --env mode=production apiUrl=https://react-training-json-server.vercel.app/",
		"build:dev": "webpack --env mode=development",
		"build:storybook": "storybook build -c ./config/storybook -s ../../public",
		"lint:ts": "eslint \"**/*.{ts,tsx}\"",
		"lint:ts:fix": "eslint \"**/*.{ts,tsx}\" --fix",
		"lint:scss": "npx stylelint \"**/*.scss\"",
		"lint:scss:fix": "npx stylelint \"**/*.scss\" --fix",
		"test:unit": "jest --config ./config/jest/jest.config.ts",
		"test:ui": "npx loki test",
		"test:ui:ok": "npx loki approve",
		"test:e2e": "npx cypress open",
		"prepare": "husky install",
		"storybook": "storybook dev -p 6006 -c ./config/storybook -s ../../public",
		"generate:slice": "node ./scripts/generateSlice/index.js",
		"postinstall": "",
		"remove-feature": "ts-node ./scripts/remove-feature.ts"
	},
	"lint-staged": {
		"**/*.{ts,tsx}": "eslint"
	},
